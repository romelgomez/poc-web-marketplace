    "compression": "^1.7.4",
    "express": "^4.19.2",
    "express-rate-limit": "^7.3.1",
    "graphql": "^16.8.2",
    "graphql-type-json": "^0.3.2",
    "helmet": "^7.1.0",
    "jwks-rsa": "^3.1.0",
    "meilisearch": "^0.40.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
